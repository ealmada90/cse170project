<!doctype html>

<html>
<head>
	<title>Power Upp</title>	
	<meta charset="utf-8">
	<meta name="viewport" content="width=device-width, initial-scale=1.0">
	
	<link rel="stylesheet" href="http://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/css/bootstrap.min.css">
  <link href='https://fonts.googleapis.com/css?family=Anton' rel='stylesheet' type='text/css'>
  <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.12.0/jquery.min.js"></script>
  <script src="http://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/js/bootstrap.min.js"></script>

	<link rel="stylesheet" href="/css/bootstrap.css">
	<link rel="stylesheet" href="/css/bootstrap-theme.css">

	<script src="/js/jquery-1.11.0.js"></script>
	<script src="/js/bootstrap.js"></script>
	<script src="/js/powerupp.js"></script>
    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
      <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
      <script src="https://oss.maxcdn.com/libs/respond.js/1.3.0/respond.min.js"></script>
    <![endif]-->        
</head>

<body>

<div class="container-fluid">
	<div class="page-header">
  		<h1 style = "text-align: center;">POWER UPP </h1>
	</div>
	<a href = "/trainer" type="button" class="btn btn-default">
      <span class="glyphicon glyphicon-circle-arrow-left" aria-hidden="true"></span> Go Back
    </a>

	<div class="row">
	  <div class="col-md-5"></div>
	  <div class="col-md-4">
	  	<h1 style: "text-align:Center">Trainer</h1> 
	  </div>
	  <div class="col-md-3"></div>
	</div>
	<div class="row">
		  <div class="col-md-3"></div>
		  <div class="col-xs-3 col-md-1">
		    <h4>Rank:</h4>
		  </div>
		  <div class="col-xs-6 col-md-6">
			  <h2 style = "text-align:center">Beginner</h2>
		  </div>
		</div>

		
	<h1 style = "text-align: center;">Find a Trainer</h2>
	<form role="form" method="get" action="/list">
	  <div class="form-group">
	    <label for="day">Day</label>
	    <select class="form-control" name = "day">
		  <option>Sunday</option>
		  <option>Monday</option>
		  <option>Tuesday</option>
		  <option>Wednesday</option>
		  <option>Thursday</option>
		  <option>Friday</option>
		  <option>Saturday</option>
		</select>
	  </div>
	  <div class="form-group">
	    <label for="location">Location</label>
	    <select class="form-control" name = "location">
		  <option>Rimac Gym</option>
		  <option>Main Gym</option>
		  <option>Warren Gym</option>
		  <option>Track</option>
		</select>
	  </div>
	  <div class = "form-group">
	  	<label for = "focus">Focus</label>
		  <div class="checkbox">
		    <label>
		      <input type="checkbox" name = "armS"> Arm Strength
		    </label>
		  </div>
		  <div class="checkbox">
		    <label>
		      <input type="checkbox" name = "torsoS"> Torso Strength
		    </label>
		  </div>
		  <div class="checkbox">
		    <label>
		      <input type="checkbox" name = "legS"> Leg Strength
		    </label>
		  </div>
		  <div class="checkbox">
		    <label>
		      <input type="checkbox" name = "armE"> Arm Endurance
		    </label>
		  </div>
		  <div class="checkbox">
		    <label>
		      <input type="checkbox" name = "torsoE"> Torso Endurance
		    </label>
		  </div>
		  <div class="checkbox">
		    <label>
		      <input type="checkbox" name = "legE"> Leg Endurance
		    </label>
		  </div>
		</div>

	  <button type="submit" class="btn btn-default btn-lg center-block" style = "text-align: center">Search</button>
	</form>

	<br><br><br><br><br>
	

	
	<div class = "row">
		<div class="col-md-2"></div>
		<div class="col-md-8">
				<nav class = "navbar-fixed-bottom">
				<div class = "container-fluid">
					<div class="btn-group btn-group-justified btn-group-lg">
					  	<a href="/home" class="btn btn-primary">
					  	<span class="glyphicon glyphicon-home" aria-hidden="true"></span>
						</a>
						<a href="/trainer" class="btn btn-primary active">
						<span class="glyphicon glyphicon-scale" aria-hidden="true"></span>
						</a>
						<a href="/stats" class="btn btn-primary">
						  <span class="glyphicon glyphicon-stats" aria-hidden="true"></span>
						</a>
						<!--a href="#" class="btn btn-primary">
						  	<span class="glyphicon glyphicon-certificate" aria-hidden="true"></span>
						</a-->
						<a href="/avatar" class="btn btn-primary">
						  	<span class="glyphicon glyphicon-user" aria-hidden="true"></span>
						</a>
					</div>
				</div>
				</nav>
			</div>
	</div>
	<br>
</div>


</body>

</html>
